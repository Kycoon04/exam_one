{
    "name": "My Ty",
    "version": "1.0.0",
    "manifest_version": 3,
    "action":{
      "default_popup": "index.html"
    },
    "permissions": [
      "scripting"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://gemini.google.com/app"],
        "js": ["content.js"]
      }
    ]
}